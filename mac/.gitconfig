[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = jamesprime
	email = jamesprimephd@gmail.com
[alias]
	co = checkout
	cm = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[core]
	autocrlf = false
	safecrlf = true
[credential]
	helper = osxkeychain

# FROM BASH-IT AUTOGENERATED .GITCONFIG FILE:
[color]
	# Use colors in Git commands that are capable of colored output when outputting to the terminal
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
#[color "status"]
	#added = yellow
	#changed = green
	#untracked = cyan
