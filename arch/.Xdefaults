!!!!!! XConfig file !!!!!!

!!! ROFI !!!
! ! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   argb:00000000,  #D8DEE9 , argb:00000000,  #FAC863 ,  #000000
! ! Color scheme for urgent row
rofi.color-urgent:                   argb:00000000,  #F99157 , argb:00000000,  #F99157 ,  #000000
! ! Color scheme for active row
rofi.color-active:                   argb:00000000,  #000000 , argb:00000000,  #000000 ,  #000000
! ! Color scheme window
rofi.color-window:                   argb:ee222222,  #FAC863 ,  #FAC863
! ! Separator style (none, dash, solid)
rofi.separator-style:                dash

!!! URxvt-unicode !!!
! Terminal
URxvt*termName: screen-256color
! Real Transparency
urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c800
! Fake Transparency
!urxvt*transparent : true
!urxvt*shading : 30
! ^^ Previously 5 (lower numbers mean darker shading)
! Font
urxvt*font: xft:DejaVu Sans Mono:size=10:antialias=false		
urxvt*boldFont: xft:DejaVu Sans Mono:size=10:antialias=false
urxvt*italicFont: xft:DejaVu Sans Mono:size=10:antialias=false
urxvt*bolditalicFont: xft:DejaVu Sans Mono:size=10:antialias=false
! Scroll
urxvt*scrollBar: false
! Settings
urxvt*internalBorder: 3
URxvt*cursorUnderline: true
URxvt*cursorBlink: true
URxvt*savelines: 10000
! Optional settings
URxvt.letterSpace:-1
! Perl extensions
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.colorBD: white
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,matcher
URxvt*perl-ext:
URxvt*perl-ext-common:

