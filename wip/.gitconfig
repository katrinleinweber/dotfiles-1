[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = jamesprime
	email = jamesprimephd@gmail.com
[alias]
	co = checkout
	cm = commit
	st = status
	br = branch
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[core]
	autocrlf = false
	safecrlf = true
[credential]
	helper = osxkeychain
